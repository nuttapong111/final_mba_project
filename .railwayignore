# Railway ignore file
# Files and directories that Railway should ignore during deployment

# Dependencies (will be installed by Railway)
node_modules/

# Build outputs (will be built by Railway)
dist/
build/
.next/
out/

# Environment files (use Railway environment variables instead)
.env
.env.local
.env.*.local

# Test files
coverage/
*.test.ts
*.test.js
*.spec.ts
*.spec.js
tests/

# Development files
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
Thumbs.db

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Temporary files
*.tmp
.cache/

# Database files
*.db
*.sqlite

# Mobile (not needed for web deployment)
mobile/

